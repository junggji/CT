package Codingtest;

public class Ct2 {

    public static String solution(String my_string, int n) {
        String answer = "";
        answer = my_string.substring(my_string.length()-n);  // substing --> ~번째 부터 문자열출력

        return answer;
    }

    public static void main(String[] args) {
        System.out.println(solution("programmerS123", 11));


    }
}
