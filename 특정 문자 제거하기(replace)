package Codingtest;

import java.util.Arrays;

public class Ct4 {

    public static String solution(String my_string, String letter) {
        String answer = "";

        answer = my_string.replace(letter, "");

        return answer;
    }

        public static void main (String[]args){
            System.out.println(solution("abcdef", "f"));

        }
    }
